//um resultado logico utilizara a estrutura if e else quando tem uma opçao de escolha se usa swetch case
//exemplo
var media = 8
if(media >=6){
    console.log ("voce passou")
}else if (media >= 4 && media <6){
        console.log("voce esta de recuperacao")
}else{
    console.log("reprovou")         //a swetch case nao iria funcionar nesse tipo de situaçao
}

